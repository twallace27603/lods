{
  "if": {
    "not": {
      "anyOf": [
        {
          "source": "action",
          "like": "Microsoft.Relay/namespaces/HybridConnections/*"
        },
        {
          "source": "action",
          "like": "Microsoft.Storage/*"
        }
      ]
    }
  },
  "then": {
    "effect": "deny"
  }
}
